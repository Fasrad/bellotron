%!
    %% pattern for bellows generated by bellofy.pl
    /mm {360 mul 127 div} def % cause I don't do points
    
    %move to center of paper based on heuristic estimate of bellows extant
    100 100 moveto
    
    0, 210 rline
    stroke 
    0, -210 rmove
    
    newpath
    0 0 moveto
    0 120 lineto
    120 120 lineto
    120 0 lineto
    0 0 lineto
    10 10 translate
    0 0 moveto
    0 100 lineto
    100 100 lineto
    100 0 lineto
    0 0 lineto

    -10 -10 translate
    0 0 moveto
    0 100 lineto
    100 100 lineto
    100 0 lineto
    0 0 lineto
    10 10 translate
    0 0 moveto
    0 120 lineto
    120 120 lineto
    120 0 lineto
    0 0 lineto

    2 setlinewidth
    stroke
    
    showpage 
    